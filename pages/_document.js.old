import Document, { Html, Head, Main, NextScript } from 'next/document';

export default class DocTemplate extends Document {
  render() {
    return (
      <Html lang="en-GB">
        <Head>
          <meta
            name="viewport"
            content="width=device-width, initial-scale=1.0"
          />
          <meta charSet="utf-8" />
        </Head>
        <body>
          <Main />
          <NextScript />
        </body>
      </Html>
    );
  }
}
